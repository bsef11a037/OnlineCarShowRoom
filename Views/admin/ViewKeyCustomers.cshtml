<head>
    <title>Car Show Room</title>

    @Scripts.Render("~/bundles/jquery")
    <script>

        $(document).ready(function () {
            
            $('#tb').empty();
            $.getJSON('/admin/getAllUsers', function (data) {

                $.each(data, function (i, item) {

                    if(item.bookings!=0)
                    $('#tb').append('<tr><td>' + item.Id + '</td><td>' + item.Name + '</td><td>' + item.city + '</td><td>' + item.bookings + '</td></tr>');

                });

               
            });
        });
    </script>
</head>



